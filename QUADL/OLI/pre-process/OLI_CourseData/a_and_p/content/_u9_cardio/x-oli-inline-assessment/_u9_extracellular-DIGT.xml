<assessment id="_u9_extracellular-DIGT">
    <title>tutor</title>
    <page>
        <question id="u9_extracellular_1">
            <!--LO:identify_different_extracellular_matrix_proteins_OBJ-->
            <body>Which extracellular matrix protein allows for blood vessels to return to their original shape?</body>
            <multiple_choice shuffle="true" select="single">
                <choice value="A">Elastin</choice>
                <choice value="B">Collagen</choice>
            </multiple_choice>
            <part>
                <response match="A" score="10">
                    <feedback>Correct. Elastin molecules are disorganized but recoil back after stretching.</feedback>
                </response>
                <response match="B" score="0">
                    <feedback>Incorrect. Collagen is stiff and prevents rupture.</feedback>
                </response>
            </part>
        </question>
        
        <question id="u9_extracellular_2">
            <!--LO:identify_different_extracellular_matrix_proteins_OBJ-->

            <body>Which extracellular proteins are found in the tunica externa?</body>
            
            <multiple_choice shuffle="true" select="single">
                <choice value="A">Elastin</choice>
                <choice value="B">Collagen</choice>
                
            </multiple_choice>
            <part>
                <response match="A" score="0">
                    <feedback>Incorrect. Elastin concentrated in the elastic laminae.</feedback>
                </response>
                <response match="B" score="10">
                    <feedback>Correct. Collagen is located in the tunicia externa and provides stiffness. </feedback>
                </response>
                
            </part>
        </question>
      
    </page>
</assessment>
