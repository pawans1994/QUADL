<?xml version="1.0" encoding="UTF-8"?>
<simulation id="EnzymeKinetics" version="2" date="10/1/05">
    <name>&lt;b&gt;Enzyme Kinetics V3&lt;/b&gt;</name>
    <intro filename=""/>
    <debugger state="on"/> 
    <steps>
              <step id="createDiana">
            <title>Enzyme Kinetics</title>
            <text>
            </text>
            <actions>
                <action event="record light"/>
               <!-- <action event="create" name="theexplainer" url="tools/text_explainer.swf"  x="2" y="30" xscale="1"  yscale="1"/> --> 
                <action event="create" name="thegrapher" url="tools/graphVariableAvgOverTime.swf" x="580" y="370" xscale="1" yscale="1" /> 
                <action event="create" name="slider1" url="tools/sliderNEW.swf"  x="310" y="450" xscale="1"  yscale=".9"/>
                <action event="create" name="beaker" url="swfs/beakerplain.swf"  x="325" y="5" xscale="2.3"  yscale="1.85"/> 
                <action event="create" name="box1" url="tools/ligandBoxKD.swf"  x="333" y="31" xscale="1"  yscale="1"/>
                <action event="create" name="equation1" url="tools/equationML1.swf"  x="320" y="400" xscale="1" yscale="1" />
                <!--<action event="create" name="equation2" url="tools/equationML2.swf"  x="470" y="400" xscale="1" yscale="1" />-->
                <action event="create" name="userdata" url="tools/userdataKD.swf"  x="350" y="290" xscale="1" yscale="1" />
                <action event="create" name="inputs1" url="tools/inputs1.swf"  x="330" y="420" xscale="1" yscale="1" />
               <!-- <action event="create" name="inputs2" url="tools/inputs2.swf"  x="495" y="420" xscale="1" yscale="1" />-->
                <action event="create" name="castKey1" url="tools/castKey1.swf"  x="330" y="375" xscale=".65" yscale=".65" />
                <action event="create" name="castKey2" url="tools/castKey2.swf"  x="500" y="375" xscale=".65" yscale=".65" />
           <!--     <action event="create" name="opener" url="swfs/ligand05.swf"  x="2" y="0" xscale="2"  yscale="2"/> -->
               <!-- <action event="create" name="LFree" url="tools/DisplayObjectWithLabel.swf"  x="595" y="320" xscale="1"  yscale="1"/>
                <action event="create" name="LIn" url="tools/DisplayAddObjectsWithLabel.swf"  x="595" y="340" xscale="1"  yscale="1"/>-->
                <action event="create" name="Toff" url="tools/VariableWithLabel.swf"  x="595" y="450" xscale="1"  yscale="1"/>                
                <action event="create" name="Kd" url="tools/VariableWithLabel2.swf"  x="595" y="390" xscale="1"  yscale="1"/> 
                <action event="create" name="ligandvelocity" url="tools/VariableWithLabel2.swf"  x="595" y="360" xscale="1"  yscale="1"/> 
                <action event="create" settimer='12' advance='yes' />
            </actions>
             <objects>
                 <object name="theexplainer" state="visible"/>
                <object name="userinput" state="hilight"/>
            </objects>
            <nextbutton text="next" state="1"/>
            <prevbutton text="Back" state="0"/>
              </step>
        <step id="login">
            <title> </title>
            <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;When you are ready to continue click "Begin Simulation."
            </text>
            
            <actions>
                <action event="record light"/>
                <action event="initialize" name="opener" />
                <action event="initialize" name="thegrapher"/> 
                <action event="initialize" name="userdata" params="" data="a-c,4.3;d-h,2.7;i-l,3.3;m-o,5.1;p-s,2.9;t-z,3.7" />
               <!-- <action event="initialize" name="LFree" data="[L] free" params="_root.inputs1.num_S.text" />
                <action event="initialize" name="LIn" data="[L] in" params="_root.inputs1.num_S.text,_root.inputs1.num_ES.text"  />-->
               <!-- <action event="start" name="Toff" data="T off" params="_root.box1.bindstrength"  />-->
                <!--needs to be loaded after box1 started-->
               <!-- <action event="start" name="Kd" data="Kd" params="10"  />-->
            </actions>
            <objects>
                <object name="opener" state="visible"/>
                <object name="beaker" state="visible"/>
                <object name="theexplainer" state="visible"/>
                <object name="userinput" state="hilight"/>
                <object name="slider1" state="invisible"/>
                <object name="thegrapher" state="visible"/> 
                <object name="equation1" state="visible" action="1" />
                <object name="equation2" state="visible" action="1" />
                <object name="castKey1" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="ligandvelocity" state="visible"/>
                <object name="thegrapher" state="invisible" >
                    <child name="curve0" state="visible" color="#FF0000" label="S=7" />
                </object>
                <object name="inputs2" capture="yes" state="visible"/>
                <object name="inputs1" capture="yes" state="visible">
                    <child name="num_E" value="10" />
                    <child name="num_S" value="40" />
                </object>
            </objects>
            <nextbutton text="Begin Simulation" state="1"/>
            <prevbutton text="Back" state="0"/>
        </step>
       
        <step id="run">
             <title> </title>
            <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;When you are finished  click "Done."
            </text>
            <objects>
                <object name="theexplainer" state="visible"/>
                <object name="box1" state="visible"/>
                <object name="beaker" state="visible"/>
                <object name="thegrapher" state="visible"/> 
                <object name="inputs1" state="visible" />
                <object name="castKey1" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="equation1" state="visible" action="1" />
                <object name="inputs2" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="equation2" state="visible" action="1" />
                <object name="LFree" state="visible"/>
                <object name="LIn" state="visible"/>
                <object name="Toff" state="visible"/>
                <object name="Kd" state="visible"/>
                <object name="ligandvelocity" state="visible"/>
             <!--   <object name="thegrapher" state="visible" />-->
                <object name="slider1" state="visible"/>
            </objects>
            <actions>
                <action event="initialize" name="box1" params="leftBorder=0,rightBorder=410,topBorder=0,bottomBorder=330,leftBorder2=0,rightBorder2=410,topBorder2=0,bottomBorder2=330,bindstrength=10,Kd=10,bindstrength2=0,mybounce=1,pause=0,makingproduct=0,eq1=inputs1,eq2=inputs2"/>
                <action event="initialize" name="Toff" data="T off" params="_root.box1.bindstrength"  />
                <action event="initialize" name="grapher"  />
                <action event="initialize" name="Kd" data="Kd" params="Kd,10"  />
                
                <action event="initialize" name="ligandvelocity" data="Kd" params="ligandVelocity1,50"  />
               <!-- <action event="initialize" name="slider1" params="L total" data="40"/>-->
                 <action event="initialize" name="slider1" params="label='L total',low=0,high=100,reduce=_root.box1.removeLigands,increase=_root.box1.addLigands" data="40"/> <!--This is what we need to wire a slider to a simulator-->
                <action event="record all"/>
            </actions>
            <nextbutton text="Done" state="1"/>
            <prevbutton text="Back" state="0"/>
        </step>
        <step id="run">
            <title> </title>
            <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;Reload the browser page to start again
            </text>
            <objects>
                <object name="theexplainer" state="visible"/>
                <object name="box1" state="visible"/>
                <object name="beaker" state="visible"/>
             <!--   <object name="thegrapher" state="visible"/> -->
                <object name="inputs1" state="visible" />
                <object name="castKey1" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="equation1" state="visible" action="1" />
                <object name="inputs2" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="equation2" state="visible" action="1" />
                <object name="LFree" state="visible"/>
                <object name="LIn" state="visible"/>
                <object name="Toff" state="visible"/>
                <object name="Kd" state="visible"/>
                <object name="ligandvelocity" state="visible"/>
                <!--   <object name="thegrapher" state="visible" />-->
                <object name="slider1" state="visible"/>
            </objects>
            <actions>
                <action event="pause"  />
                <action event="record all"/>
            </actions>
            <nextbutton text="Done" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
        

        
    </steps>
</simulation>
