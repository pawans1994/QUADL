<?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="tca6">
         <presentation>
           <material>
             <mattext texttype="text/plain">The following step produces FADH2.

&lt;b&gt;This step is a ________ involving the conversion of a ________  to a __________.&lt;/b&gt;
             
[You cannot advance to the next step until you select the correct answer.]
             </mattext></material>
           <response_lid rcardinality="Single" ident="tca6">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">oxidation, alkane, alkene</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">reduction, alkane, alkene</mattext></material></response_label>
               <response_label ident="Res3">
               <material>
                 <mattext texttype="text/plain">oxidation, alkene, alkane</mattext></material></response_label>
               <response_label ident="Res4">
                 <material>
                   <mattext texttype="text/plain">reduction, alkane, alkene</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="tca6_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca6_02">Res2</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca6_03">Res3</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca6_04">Res4</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
        </resprocessing>
         <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Is the reaction an oxidation or reduction if FADH2 is produced?</mattext></material>
             </hintmaterial> 
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">What carbons within succinate can undergo this type of redox reaction?</mattext></material>
             </hintmaterial> 
            </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack0">
           <material>
             <mattext texttype="text/plain">Correct!</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Incorrect, this is an oxidation.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Incorrect, the conversion of an alkene to an alkane is an reduction.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack3">
           <material>
             <mattext texttype="text/plain">Incorrect, the chemical change is an oxidation.</mattext></material></itemfeedback>
        </item>
     </questestinterop>
