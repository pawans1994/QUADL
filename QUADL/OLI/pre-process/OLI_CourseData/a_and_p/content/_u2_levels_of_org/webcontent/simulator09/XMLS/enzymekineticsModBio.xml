                <object name="pauseBtn" state="visible"/>
                <object name="Toff" state="visible"/>
                <object name="Kd" state="visible"/>
                <object name="ligandvelocity" state="visible"/>
                
                <object name="k" state="visible"/>
                <object name="thegrapher" state="visible" />
                <object name="slider1" state="visible"/>
            </objects>
            <actions>
                <action event="initialize" name="ligandvelocity" data="  " params="thevariable=ligandVelocity1,thelabel=ligand Velocity,thevalue="  />
                <action event="initialize" name="box1" params="leftBorder=0,rightBorder=410,topBorder=0,bottomBorder=330,leftBorder2=0,rightBorder2=410,topBorder2=0,bottomBorder2=330,k=760,Kd=50,bindstrength2=0,mybounce=1,pause=0,makingproduct=0,eq1=inputs1,eq2=inputs2"/>
