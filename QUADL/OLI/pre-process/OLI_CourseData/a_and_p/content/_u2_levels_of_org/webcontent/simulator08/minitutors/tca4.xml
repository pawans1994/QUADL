<?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="tca4">
         <presentation>
           <material>
             <mattext texttype="text/plain">The oxidation of alpha-ketoglutarate occurs
in a manner similar to the oxidation of pyruvate. 
             
&lt;b&gt;Which of the following compounds is the correct product of the oxidation of alpha-ketoglutarate?&lt;/b&gt;
             </mattext></material>
           <response_lid rcardinality="Single" ident="tca4">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">A</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">B</mattext></material></response_label>
               <response_label ident="Res3">
               <material>
                 <mattext texttype="text/plain">C</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="tca4_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca4_02">Res2</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca4_03">Res3</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
        </resprocessing>
         <itemfeedback ident="hint">
        <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">The CO2 leaves first, giving an aldehyde.</mattext></material>
             </hintmaterial> 
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">The aldehyde is then oxidized to a thioester.</mattext></material>
             </hintmaterial> 
            </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack0">
           <material>
             <mattext texttype="text/plain">Correct.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">You lost a CO2, but was it the correct one?</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Has the oxidation state changed?</mattext></material></itemfeedback>
        </item>
     </questestinterop>
