<?xml version="1.0" encoding="UTF-8"?>
<simulation id="LigandBindingExperiment" version="1" date="3/1/06">
    <name>&lt;b&gt; &lt;/b&gt;</name>
   <!-- <intro filename=""/>-->
    <debugger state="off"/> 
    <steps>
              <step id="create">

            <actions>
                <action event="record light"/> 
               <action event="create" name="button" url="tools/button.swf"  x="25" y="500" xscale="1"  yscale="1"/>
                <action event="create" name="bilayer" url="swfs/bilayer.swf"  x="0" y="180" xscale="1.2"  yscale=".4"/>
                <action event="create" name="bilayer2" url="swfs/bilayer.swf"  x="0" y="240" xscale="1.2"  yscale=".4"/>
            <!--    <action event="create" name="bilayer" url="swfs/bilayer-still.jpg"  x="0" y="190" xscale="1.2"  yscale="1"/>-->
                <action event="create" name="box1" url="tools/ligandBoxKDNEW.swf"  x="0" y="0" xscale="1"  yscale="1"/> 
                <action event="settimer" timer='12' advance='yes' />
            </actions>
             <objects>
                 <object name="theexplainer" state="visible"/>
                 <title>Equilibrium</title>
                 <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;Equilibrium"
                 </text>
                <object name="userinput" state="hilight"/>
            </objects>
                  <nextbutton text="Begin Simulation" />
            <prevbutton text="Back" state="0"/>
              </step>
      
        <step id="startmolecules">
            <objects>
                <object name="theexplainer" state="visible">
                    <title>Watch as the glucose passes through</title>
                    <text> </text>
                </object>
                <object name="bilayer" state="visible"/>
                <object name="bilayer2" state="visible"/>
                <object name="box1" state="visible"/>
                <object name="button" state="visible"/>
            </objects>
            <actions> 
                <action event="initialize" name="box1" params="leftBorder=100,rightBorder=550,topBorder=0,bottomBorder=500,leftBorder2=000,rightBorder2=550,topBorder2=180,bottomBorder2=260">
                    <molecules>
                        <molecule numMolecules= "0" myID="0"/>     <!--This is a fudge if only one molecule seems to need to be here-->  
                        <molecule numMolecules= "20"  injectionforce= "0" velocity= "30"  cast= "moleculewater"  eq1="500" eq2="200"  angle1= "30" angle2= "315" x="250" y="80" Xoffset="30" Yoffset="30" rotationVelocity="0"  rotation="0"     polarity = "0,0,0,0,.5,.5,.5,.5" mytype="ligand"  myID="2"   bindswith="molecule1.pocket1.bound1,Kd?5,k?500" mybounce="true" />
                        <molecule numMolecules= "10"  injectionforce= "0" velocity= "40"  cast= "glucose"  eq1="500" eq2="200"  angle1= "30" angle2= "315" x="250" y="100" Xoffset="30" Yoffset="30" rotationVelocity="50"  rotation="50"  polarity = "0,0,0,0,0,0,0,0"  mytype="ligand"  myID="3"  bindswith="molecule1.pocket2.bound2,Kd?10,k?500"  mybounce="true" />     
                        <molecule numMolecules= "2"  injectionforce= "0" velocity= "0"  cast= "GapJunction"  eq1="500" eq2="200"  angle1= "0" angle2= "0" x="200" y="150" Xoffset="225" Yoffset="0" Xalign="true"  rotationVelocity="0"  rotation="0"   transporter="1" polarity = "1,1,1,1,1,1,1,1" mytype="molecule"  myID="1"   mybounce="true" />  
                    </molecules>
                </action>
                <action event="record all"/>
            </actions>
            <nextbutton text="Done" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
   
        
    </steps>
</simulation>
