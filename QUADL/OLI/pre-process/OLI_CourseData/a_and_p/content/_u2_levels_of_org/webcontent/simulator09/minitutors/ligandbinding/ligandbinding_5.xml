 <?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="ligandbinding_5">
         <presentation>
           <material>
             <mattext texttype="text/plain">&lt;b&gt;As the number of ligand, in this case oxygen, &lt;br&gt;is added the graph responds how?&lt;/b&gt;  </mattext></material>
           <response_lid rcardinality="Single" ident="LID_01">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">Continues to increase with no limit</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">Decreases with no limit.</mattext></material></response_label>
               <response_label ident="Res3">
                 <material>
                   <mattext texttype="text/plain">Levels off at a set point around 1</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res2</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res3</varequal>
             </conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/>
           </respcondition>
        </resprocessing>
         <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Looking at the tail end of the graph, what happens to these Y values?</mattext></material>
             </hintmaterial>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Try and remember what the simulation looked like as you added O and what that means in terms of the graph.</mattext></material>
             </hintmaterial>
              </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Incorrect.  Eventually the ligand will reach a saturation point and the graph should level off</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Incorrect. As you add ligand you would be increasing the value of Y.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack3">
           <material>
             <mattext texttype="text/plain">Correct. Eventually the ligand will saturate the enzyme and the Y value will level off.</mattext></material></itemfeedback>
        </item>
     </questestinterop>
