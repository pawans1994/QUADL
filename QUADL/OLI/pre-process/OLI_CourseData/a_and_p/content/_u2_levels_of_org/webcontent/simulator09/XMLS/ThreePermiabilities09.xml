<?xml version="1.0" encoding="UTF-8"?>
<simulation id="LigandBindingExperiment" version="1" date="3/1/06">
    <name>&lt;b&gt; &lt;/b&gt;</name>
    <intro filename=""/>
    <debugger state="off"/> 
    <blueback state="off"/>
    <steps>
              <step id="create">

            <actions>
                <action event="record light"/> 
                <action event="create" name="bilayer" url="swfs/bilayer.swf"  x="0" y="190" xscale="1.2"  yscale="1.2"/>
                <action event="create" name="box1" url="tools/ligandBox09.swf"  x="0" y="0" xscale="1"  yscale="1"/> 
                <action event="create" name="button" url="tools/buttonLIGHT.swf"  x="25" y="450" xscale="1"  yscale="1"/>
                <action event="settimer" timer='9' advance='yes' resettimer='0' />
            </actions>
            <objects tutor="">
                 <object name="theexplainer" state="visible"/>
                 <title>Equilibrium</title>
                 <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;Equilibrium"
                 </text>
                <object name="userinput" state="hilight"/>
            </objects>
                  <nextbutton text="Begin Simulation" state="0"/>
                  <prevbutton text="Back" state="0"/>
              </step>
        <step id="begin">
            <actions>
                <action event="record light"/> 
                 <action event="initialize" name="button" params="" data="Begin Simulation"/>
            </actions>
            <objects tutor="">
                <title>Passive Diffusion</title>
                <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;
                </text>
                <object name="button" state="visible"/>
                <object name="button" state="visible"/>
            </objects>
            <nextbutton text="Add Ink"  state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>  
        <step id="startmolecules">
            <objects tutor="">               
                <object name="bilayer" state="visible"/>
                <object name="box1" state="visible"/>
                <object name="button" state="visible"/>
            </objects>
            <actions> 
                <action event="initialize"  name="button" params="object=box1,action=reset"  data="Reset"/>  
                <action event="initialize" name="box1" params="leftBorder=0,rightBorder=750,topBorder=0,bottomBorder=500,leftBorder2=0,rightBorder2=750,topBorder2=200,bottomBorder2=280">
                    <molecules>
                        <molecule numMolecules= "0" myID="0"/>     <!--This is a fudge if only one molecule seems to need to be here-->  
                        <molecule numMolecules= "5"  injectionforce= "0" velocity= "60"  cast= "moleculeNAplus"  eq1="400" eq2="200"  angle1= "30" angle2= "355" x="350" y="80" Xoffset="30" Yoffset="30" rotationVelocity="0"  rotation="10"  watchEnvironment= "0" brownian=".01" polarity = "0,0,0,0,0,0,0,0" mytype="molecule"  myID="1"  mybounce="true" />             
                        <molecule numMolecules= "10"  injectionforce= "0" velocity= "50"  cast= "ligandblue"  eq1="400" eq2="200"  angle1= "30" angle2= "315" x="350" y="80" Xoffset="30" Yoffset="30" rotationVelocity="0"  rotation="0"  watchEnvironment= "0" brownian=".05" polarity = "0,0,0,0,.95,.01,.01,1" mytype="molecule"  myID="2"  mybounce="true" />
                        <molecule numMolecules= "20"  injectionforce= "0" velocity= "40"  cast= "moleculewater"  eq1="400" eq2="200"  angle1= "30" angle2= "315" x="350" y="100" Xoffset="30" Yoffset="30" rotationVelocity="50"  rotation="50"  watchEnvironment= "0" brownian=".05" polarity = "0,0,0,0,.8,1,1,.8"  mytype="ligand"  myID="3"  mybounce="true" />   
                        <molecule numMolecules= "20"  injectionforce= "0" velocity= "40"  cast= "moleculewater"  eq1="400" eq2="200"  angle1= "30" angle2= "315" x="350" y="380" Xoffset="30" Yoffset="30" rotationVelocity="50"  rotation="50"  watchEnvironment= "0" brownian=".05" polarity = "0,0,0,0,.8,1,1,.8"  mytype="ligand"  myID="4"  mybounce="true" />   
                    </molecules>
                </action>
                <action event="continue"/>
                <action event="record all"/>
            </actions>
            <nextbutton text="Done" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
        <step id="done">
            <objects>
                <object name="theexplainer" state="visible">
                    <title> </title>
                    <text>&lt;br&gt;&lt;br&gt;&lt;br&gt;Reload the browser page to start again
                    </text>              
                </object>
                <object name="box1" state="visible"/>
                <object name="beaker" state="visible"/>
                <object name="inputs1" state="visible" />
                <object name="castKey1" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="equation1" state="visible" action="1" />
                <object name="inputs2" state="visible" />
                <object name="castKey2" state="visible" />
                <object name="equation2" state="visible" action="1" />
                <object name="LFree" state="visible"/>
                <object name="LIn" state="visible"/>
                <object name="Toff" state="visible"/>
                <object name="Kd" state="visible"/>
                <!--   <object name="thegrapher" state="visible" />-->
                <object name="slider1" state="visible"/>
            </objects>
            <actions>
                <action event="pause"  />
                <action event="record all"/>
            </actions>
            <nextbutton text="Done" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
        
    </steps>
</simulation>
