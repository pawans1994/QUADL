                <action event="create" name="slider2" url="tools/verticalSlider2.swf"  x="505" y="420" xscale="1" yscale="1" />
                <action event="create" name="castKey1" url="swfs/catalysisKeyArrows.swf"  x="406" y="370" xscale=".7" yscale=".65" />
                <action event="create" name="label1" url="tools/label.swf"  x="340" y="425" xscale=".8" yscale=".8" />
                <action event="create" name="arrow" url="swfs/arrowRight.swf"  x="365" y="430" xscale=".8" yscale=".6" />
