                <action event="create" name="equation2" url="tools/equation2.swf"  x="510" y="400" xscale="1" yscale="1" />
                   <action event="create" name="userdata" url="tools/userdataKD.swf"  x="350" y="290" xscale="1" yscale="1" />-->
                <action event="create" name="inputs1" url="tools/DisplayValue.swf"  x="280" y="440" xscale=".8" yscale=".8" />
                <action event="create" name="inputs2" url="tools/DisplayValue.swf"  x="345" y="440" xscale=".8" yscale=".8" />
