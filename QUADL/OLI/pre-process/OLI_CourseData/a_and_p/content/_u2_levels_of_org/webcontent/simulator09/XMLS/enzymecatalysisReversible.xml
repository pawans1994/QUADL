                <!--<action event="create" name="castKey2" url="tools/castKey2.swf"  x="540" y="375" xscale=".65" yscale=".65" />-->
             <!--   <action event="create" name="opener" url="swfs/ligand05.swf"  x="2" y="0" xscale="2"  yscale="2"/> -->
