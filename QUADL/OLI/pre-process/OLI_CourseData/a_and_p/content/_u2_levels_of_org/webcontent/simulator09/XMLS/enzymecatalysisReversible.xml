<?xml version="1.0" encoding="UTF-8"?>
<simulation id="EnzymeCatalysis-4 Enzymes" version="1" simulatorVersion="06" date="02/12/06">
    <name>&lt;b&gt;Enzyme Catalysis Pathway&lt;/b&gt;</name>
