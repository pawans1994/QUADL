 <?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="inhibitors3">
         <presentation>
           <material>
             <mattext texttype="text/plain">How would you describe the relationship
between [S] and velocity at very low substrate:</mattext></material>
           <response_lid rcardinality="Single" ident="LID_01">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">linear (v  = m [S])</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">quadratic (v = m [S][S])</mattext></material></response_label>
               <response_label ident="Res3">
               <material>
                 <mattext texttype="text/plain">hyperbolic v = m [S]/(K+[S])</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_02">Res2</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_03">Res3</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
        </resprocessing>
         <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Hint1</mattext></material>
             </hintmaterial>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Hint2 </mattext></material>
             </hintmaterial>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Hint3 
                 </mattext>
               </material>
             </hintmaterial>            
            </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack0">
           <material>
             <mattext texttype="text/plain">Correct.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
        </item>
     </questestinterop>
