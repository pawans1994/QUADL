 <?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="endocytosis-006">
         <presentation>
           <material>
             <mattext texttype="text/plain">Signal transduction involves all of the following except for which of the following items</mattext></material>
           <response_lid rcardinality="Single" ident="LID_01">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">GTP/GDP exchange</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">enzyme activation</mattext></material></response_label>
               <response_label ident="Res3">
                 <material>
                   <mattext texttype="text/plain">pH control of conformational change pH control of conformational change</mattext></material></response_label>
               <response_label ident="Res4">
                 <material>
                   <mattext texttype="text/plain">ligand recognition by a specific receptor</mattext></material></response_label></render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res2</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res3</varequal></conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res4</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition></resprocessing>
         <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">The G-protein/GDP complex binds to the intra-celluar part of the receptor-ligand complex.</mattext></material>
             </hintmaterial>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">The ligand forms a specific complex with its receptor. </mattext></material>
             </hintmaterial>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">The binding of ligand to the receptor causes an allosteric change in the structure of the receptor on the other side of the membrane, inside the cell. 
                 </mattext>
               </material>
             </hintmaterial>            
            </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack0">
           <material>
             <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Correct.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack3">
           <material>
             <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack4">
           <material>
             <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback></item>
     </questestinterop>
