<?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="tca5">
         <presentation>
           <material>
             <mattext texttype="text/plain">The thioester on succinyl-CoA is hydrolyzed
here, releasing CoA and forming the four carbon dicarboxylic acid succinate.
             
&lt;b&gt;What is produced in addition to succinate?&lt;/b&gt;

[You cannot advance to the next step until you select the correct answer.]
             </mattext></material>
           <response_lid rcardinality="Single" ident="tca5">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">ATP</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">GTP</mattext></material></response_label>
               <response_label ident="Res3">
               <material>
                 <mattext texttype="text/plain">NADH</mattext></material></response_label>
               <response_label ident="Res4">
                 <material>
                   <mattext texttype="text/plain">FADH2</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="tca5_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca5_02">Res2</varequal></conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca5_03">Res3</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="tca5_04">Res4</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
        </resprocessing>
         <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Thioesters are high energy compounds. All four of the answers are compounds capable of storing the energy.  To narrow the choices, decide whether this is a redox reaction or not.
                 </mattext></material>
             </hintmaterial> 
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Thioesters are the same oxidation state as carboxylic acids.
                 </mattext></material>
             </hintmaterial>        
            </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack0">
           <material>
             <mattext texttype="text/plain">Almost correct, but another trinucleotide is made.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Correct!</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Incorrect, this is not an oxidation.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack3">
           <material>
             <mattext texttype="text/plain">Incorrect, this is not an oxidation.</mattext></material></itemfeedback>
        </item>
     </questestinterop>
