<?xml version="1.0" encoding="UTF-8"?>
<simulation id="equilibrium dialysis" version="1" simulatorVersion="09" date="12/15/08">
    <name>&lt;b&gt;Enzyme Catlysis&lt;/b&gt;</name>
    <intro filename=""/>
    <debugger state="off"/>
    <blueback state="off"/>
    <steps>
         <step id="create">
            <actions>
                <action event="record light"/>                
                <action event="create" name="box1" url="tools/ligandBox09.swf"  x="0" y="0" xscale="2"  yscale="2"/>              
                <action event="create" name="button" url="tools/buttonLIGHT.swf"  x="25" y="450" xscale="1"  yscale="1"/>
                <action event="create" name="shape" url="swfs/box.swf"  x="0" y="0" xscale="2"  yscale="2"/>
                <action event="settimer" timer='9' advance='yes' resettimer='0' />
            </actions>
             <objects tutor="">
                 <object name="button" state="invisible"/>
                 <object name="box1" state="invisible"/>
             </objects>
             <nextbutton text="Begin Simulation" state="0"/>
             <prevbutton text="Back" state="0"/>
         </step>
        <step id="wait">
            <actions>
                <action event="record light"/>                
                <action event="initialize" name="button" params="" data="Start"/>
            </actions>
            <objects tutor="">
                <object name="button" state="visible"/>
                <object name="box1" state="invisible"/>
            </objects>
            <nextbutton text="Begin Simulation" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
        <step id="dialysis">
            <objects tutor="">
                <object name="button" state="visible"/>
                <object name="box1" state="visible"/>
                <object name="shape" state="visible"/>
            </objects>
            <actions>
                <action event="reset" name="box1" params="3"/>
                <action event="initialize"  name="button" params="object=box1,action=reset"  data="reset"/> 
                <action event="initialize" name="box1" params="leftBorder=0,rightBorder=375,topBorder=0,bottomBorder=250,leftBorder2=188,rightBorder2=375,topBorder2=0,bottomBorder2=250">
                    <molecules>  
                        <molecule numMolecules= "0" myID="0"/>     <!--This is a fudge if only one molecule seems to need to be here--> 
                        <molecule numMolecules= "0"  numbound= "0" velocity= "40"  cast= "enzymeBound"   angle1= "45" angle2= "315" rotationVelocity="5"  rotation="30" x="100" y="100" Xoffset="5" Yoffset="5"  mytype="molecule"  myID="1"  polarity = "0,0,0,0,0,0,0,0" mybounce="true" seperateinto="productidl?3,productidm?2,productidp?4,Kd?50,k?2500,Kcat?0"/>            
                        <molecule numMolecules= "4"  numbound= "0" velocity= "40"  cast= "enzyme" angle1= "45" angle2= "315" rotationVelocity="5"  rotation="30" x="50" y="100" Xoffset="20" Yoffset="20"  mytype="molecule"  myID="2"  polarity = "0,0,0,0,0,0,0,0" mybounce="true" />            
                        <molecule numMolecules= "12"  numbound= "0" velocity= "60"  cast= "ligandblue"   angle1= "45" angle2= "315" rotationVelocity="5"  rotation="30"   x="250" y="100" Xoffset="25" Yoffset="25" mytype="ligand"  myID="3"   polarity = ".5,.5,0,0,0,0,0,0" mybounce="true" bindswith="molecule2.pocket1.free,Kd?50,k?2500,productidm?1" />   
                        <molecule numMolecules=  "0"  numbound= "0" velocity= "60"  cast= "product" angle1= "45" angle2= "315" rotationVelocity="5"  rotation="30"   x="100" y="100" Xoffset="5" Yoffset="5" mytype="ligand"  myID="4"    polarity = "0,0,0,0,0,0,0,0" mybounce="true"  /> 
                    </molecules>
                </action>
                <action event="continue"  />
                <action event="record all"/>
            </actions>
            <nextbutton text="reset" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
        <step id="interlude">
            <objects tutor="">
                <object name="box1" state="visible"/>
                <object name="Hline" state="invisible"/>
            </objects>
            <actions>
                <action event="record light"/>
                <action event="settimer" timer='6' advance='yes' resettimer='0' />
            </actions>
            <nextbutton text="Next" state="1"/>
            <prevbutton text="Back" state="0"/>
        </step>
        <step id="prepare to exit">
            
            <objects tutor="">
                
            </objects>
            <actions>
                <action event="unload" name="box1"/>
                <action event="settimer" timer='6' advance='yes' resettimer='0' />
            </actions>
            <nextbutton text="Next" state="0"/>
            <prevbutton text="Back" state="0"/>
        </step>
        
        <step id="exit">
            
            <objects tutor="">
                
            </objects>
            <actions>
                <action event="record light"/>
                <action event="exit" />
            </actions>
        </step>
        
    </steps>
</simulation>
