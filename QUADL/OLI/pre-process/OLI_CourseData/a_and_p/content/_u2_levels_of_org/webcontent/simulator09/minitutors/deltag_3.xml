 <?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="deltag_3">
         <presentation>
           <material>
             <mattext texttype="text/plain">Leave the system in the same state. Watch 
the collection of molecules at this equilibrium point.               
               
&lt;b&gt;For any one molecule, what fraction of the &lt;i&gt;time&lt;/i&gt; is it found in form A?&lt;/b&gt;</mattext></material>
           <response_lid rcardinality="Single" ident="deltag3_01">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">0</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">.25</mattext></material></response_label>
               <response_label ident="Res3">
               <material>
                 <mattext texttype="text/plain">.5</mattext></material></response_label>
               <response_label ident="Res4">
                 <material>
                   <mattext texttype="text/plain">.75</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="deltag3_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="deltag3_02">Res2</varequal></conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="deltag3_03">Res3</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="deltag3_04">Res4</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition>
        </resprocessing>
       <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">When the molecule is in form A it is a pink hexagon.</mattext>
               </material>
             </hintmaterial>  
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Make sure you watch a molecule long enough to get an idea of its average time in both forms.</mattext>
               </material>
             </hintmaterial>  
            </hint>
         </itemfeedback> 
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Incorrect, you did not watch long enough.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Correct.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack3">
           <material>
             <mattext texttype="text/plain">Incorrect, this would be a system where form A is equally as favorable as form B.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack4">
           <material>
             <mattext texttype="text/plain">Incorrect. you should be taking note of how long it is in form A.</mattext></material></itemfeedback>
        </item>
     </questestinterop>
