 <?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
       <item ident="ligandbinding_6">
         <presentation>
           <material>
             <mattext texttype="text/plain">&lt;b&gt;Would you expect this ligand to bind tightly or weakly?&lt;/b&gt;  </mattext></material>
           <response_lid rcardinality="Single" ident="LID_01">
             <render_choice>
               <response_label ident="Res1">
                 <material>
                   <mattext texttype="text/plain">Tightly</mattext></material></response_label>
               <response_label ident="Res2">
                 <material>
                   <mattext texttype="text/plain">Weakly</mattext></material></response_label>
               </render_choice>
           </response_lid>
         </presentation>
         <resprocessing>
           <outcomes>
             <decvar varname="SCORE" vartype="integer"/></outcomes>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res1</varequal>
             </conditionvar>
             <setvar varname="SCORE">10</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/>
           </respcondition>
           <respcondition>
             <conditionvar>
               <varequal respident="LID_01">Res2</varequal></conditionvar>
             <setvar varname="SCORE">0</setvar>
             <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
        </resprocessing>
         <itemfeedback ident="hint">
           <hint>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">Relate what you see in the graph to the dissociation constant.</mattext></material>
             </hintmaterial>
             <hintmaterial>
               <material>
                 <mattext texttype="text/plain">The Kd is equal to the point where Y = 0.5</mattext></material>
             </hintmaterial>
              </hint>
         </itemfeedback>
         <itemfeedback ident="FeedBack1">
           <material>
             <mattext texttype="text/plain">Correct. Since the Kd is small the ligand must bind tightly.</mattext></material></itemfeedback>
         <itemfeedback ident="FeedBack2">
           <material>
             <mattext texttype="text/plain">Incorrect. This is only true if the Kd is large, which it is not in this example.</mattext></material></itemfeedback>
        </item>
     </questestinterop>
