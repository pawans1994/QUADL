    <title>tutor</title>
    <page>
        <question id="q1">
            <!--LO:skeletal_homeo_bone-->
            <body>Besides milk what are some foods that are high in calcium?</body>
            <multiple_choice shuffle="true" select="single">
                <choice value="A">beef</choice>
                <choice value="B">fresh Fruit</choice>
                <choice value="C">dark green vegetables</choice>
                <choice value="D">fresh sea food</choice>
            </multiple_choice>
            <part>
                <response match="A" score="0">
                    <feedback>Incorrect.  Beef is a good source of protein.</feedback>
                </response>
                <response match="B" score="0">
                    <feedback>Incorrect.  Fresh fruit is a good source of Vitamin C.</feedback>
                </response>
                <response match="C" score="10">
                    <feedback>Correct.  Dark green vegetables are  high in calcium.</feedback>
                </response>
                <response match="D" score="0">
                    <feedback>Incorrect. Fresh sea food is high in protein.</feedback>
                </response>
            </part>
        </question>
