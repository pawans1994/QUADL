<assessment id="endo_repro_female_digt">
    <title>tutor</title>
    <page>
        <question id="q1">
            <!--LO:endo_repro_female_digt-->
            <body>The secondary sex characteristics in female are produced by:</body>
            <multiple_choice shuffle="true" select="single">
                <choice value="A">FSH</choice>
                <choice value="B">LH</choice>
                <choice value="C">Estrogen</choice>
                <choice value="D">Progesterone</choice>
            </multiple_choice>
            <part>
                <response match="A" score="0">
                    <feedback>Incorrect. FSH stimulates development of support cells called follicles.</feedback>
                </response>
                <response match="B" score="0">
                    <feedback>Incorrect.  LH regulates development and release of the egg.</feedback>
                </response>
                <response match="C" score="10">
                    <feedback>Correct.  Estrogen produces the secondary sex characteristics in female.</feedback>
                </response>
                <response match="D" score="0">
                    <feedback>Incorrect.  Progesterone helps to regulate the menstrual cycle.</feedback>
                </response>
            </part>
        </question>
    </page>
</assessment>