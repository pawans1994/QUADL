    <presentation>
      <material>
        <mattext texttype="text/plain">Which of the following is a correct representation of the given information in a two-way probability table?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <matimage label="" uri="../../webcontent/dig003.jpg"></matimage></material></response_label>
          <response_label ident="Res2">
