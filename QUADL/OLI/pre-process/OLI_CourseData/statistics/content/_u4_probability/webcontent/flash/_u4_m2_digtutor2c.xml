 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="_u4_m2_digtutor2c">        <presentation>          <material>            <mattext texttype="text/plain">The probability .57 represents:</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">P(D|B)</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">P(B|D)</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">P(D and not B)</mattext></material></response_label>              <response_label ident="Res4">                <material>                  <mattext texttype="text/plain">P(D|not B)</mattext></material></response_label>				  <response_label ident="Res5">                <material>                  <mattext texttype="text/plain">P(not B|D)</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res4</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"></displayfeedback></respcondition>			<respcondition>            <conditionvar>              <varequal respident="LID_01">Res5</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"></displayfeedback></respcondition>          </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Incorrect. P(D|B)=P(D and B)/P(B)=.38/.40</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect. P(B|D)=P(B and D)/P(D)=.38/.95</mattext></material></itemfeedback><itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Good Job. Indeed, lifting directly from the table, P(D and not B)=.57</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack3">          <material>            <mattext texttype="text/plain">Incorrect. P(D|not B)=P(D and not B)/P(not B)=.57/.60</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack4">          <material>            <mattext texttype="text/plain">Incorrect. P(not B|D)=P(not B and D)/P(D)=.57/.95</mattext></material></itemfeedback></item></questestinterop>