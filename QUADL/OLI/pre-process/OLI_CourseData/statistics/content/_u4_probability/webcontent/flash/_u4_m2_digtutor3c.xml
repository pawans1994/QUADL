<?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
     <item ident="stats_item_001.2" title="_u4_m2_digtutor3c">
        <presentation>
          <material>
            <mattext texttype="text/plain">From the table, we also see that P(D) is equal to which of the following?</mattext></material>
          <response_lid rcardinality="Single" ident="LID_01">
            <render_choice>
              <response_label ident="Res1">
                <material>
                  <mattext texttype="text/plain">0.40</mattext></material></response_label>
              <response_label ident="Res2">
                <material>
                  <mattext texttype="text/plain">0.60</mattext></material></response_label>
              <response_label ident="Res3">
                <material>
                  <mattext texttype="text/plain">0.95</mattext></material></response_label>
              <response_label ident="Res4">
                <material>
                  <mattext texttype="text/plain">0.05</mattext></material></response_label>
              </render_choice></response_lid></presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res1</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res2</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res3</varequal></conditionvar>
            <setvar varname="SCORE">10</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res4</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"></displayfeedback></respcondition>
         </resprocessing>

        <itemfeedback ident="FeedBack0">
          <material>
            <mattext texttype="text/plain">Incorrect. 0.40 is P(B) not P(D).</mattext></material></itemfeedback>
        <itemfeedback ident="FeedBack1">
          <material>
            <mattext texttype="text/plain">Incorrect. 0.60 is P(not B), not P(D).</mattext></material></itemfeedback>
<itemfeedback ident="FeedBack2">
          <material>
            <mattext texttype="text/plain">Correct. P(D) = 0.95</mattext></material></itemfeedback>
        <itemfeedback ident="FeedBack3">
          <material>
            <mattext texttype="text/plain">Incorrect. 0.05 = P(not D).</mattext></material></itemfeedback>
       </item></questestinterop>
