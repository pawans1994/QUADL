 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="_u4_m2_digtutor4c">        <presentation>          <material>            <mattext texttype="text/plain">Using the table, we also find that P(D|not B) is equal to:			</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">.57</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">.57/.95=.60</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">.57/.60=.95 </mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>         </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Incorrect. .57 represents P(D and not B).</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect. .57/.95 represents P(not B|D), and you are asked to find P(D|not B).</mattext></material></itemfeedback><itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Good Job! Indeed, P(D|not B)=P(D and not B)/P(not B)=.57/.60=.95</mattext></material></itemfeedback>       </item></questestinterop>