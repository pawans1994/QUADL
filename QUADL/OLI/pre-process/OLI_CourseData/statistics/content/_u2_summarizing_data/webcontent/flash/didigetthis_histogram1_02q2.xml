<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="Histogram1of3DIGTq2" title="question_2">
    <presentation>
      <material>
        <mattext texttype="text/plain">What percentage of the students surveyed eat no more than 3 servings of fruits and vegetables daily?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">20%</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">31.2%</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">37.5%</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">62.5%</mattext></material></response_label>
          <response_label ident="Res5">
            <material>
              <mattext texttype="text/plain">68.8%</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res5</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">While it is true that 20 students eat no more than 3 servings daily, this is not a percentage. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">You have found the percentage of students who eat less than 3 servings daily. No more than 3 servings would also include those students who eat exactly 3 servings daily. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">You have found the percentage of students who eat more than 3 servings daily. The question asked for "no more than 3" servings daily. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
      <material>
        <mattext texttype="text/plain">Correct. 20/32 = 0.625 or 62.5%.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack4">
      <material>
        <mattext texttype="text/plain">You have found the percentage of students who eat 3 or more servings daily. The question asked for "no more than 3" servings daily. Please try again.</mattext></material></itemfeedback></item></questestinterop>
