<?xml version="1.0" encoding="UTF-8"?>
     <item ident="stats_item" title="didigetthis_histogram1q1">
        <hint>
          <material>
            <mattext texttype="text/plain"></mattext></material>
        </hint><presentation>
          <material>
            <mattext texttype="text/plain">What do the numbers on the horizontal axis represent?</mattext></material>
          <response_lid rcardinality="Single" ident="LID_01">
            <render_choice>
              <response_label ident="Res1">
                <material>
                  <mattext texttype="text/plain">The values of the number of hours studied</mattext></material></response_label>
              <response_label ident="Res2">
                <material>
                  <mattext texttype="text/plain">The count of students falling in each of the intervals</mattext></material></response_label>
</render_choice></response_lid></presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res1</varequal></conditionvar>
            <setvar varname="SCORE">10</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res2</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>
</resprocessing>
        <itemfeedback ident="FeedBack0">
          <material>
            <mattext texttype="text/plain">Correct.</mattext></material></itemfeedback>
        <itemfeedback ident="FeedBack1">
          <material>
            <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
</item>
