<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="BoxPlot2of3q2" title="question_2">
    <presentation>
      <material>
        <mattext texttype="text/plain">50% of the debts owed are smaller than what amount?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">$25,000</mattext></material></response_label>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">$22,000</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">$20,600</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">$18,000</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">Incorrect. $25,000 is the third quartile (Q3) of the distribution. 75% of the debts are smaller than this amount.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">Incorrect. Although $22,000 is in the middle of the box, half-way between Q1 and Q3, it is not the median.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
      <material>
        <mattext texttype="text/plain">Correct. 50% of the debts are smaller than the median, which according to the boxplot is between 20,000 and 21,000.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack4">
      <material>
        <mattext texttype="text/plain">Incorrect. $18,000 is the first quartile (Q1) of the distribution. 25% of the debts are smaller than this amount.</mattext></material></itemfeedback></item></questestinterop>
