 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="Title">        <presentation>          <material>            <mattext texttype="text/plain">50% of the shows have a rating greater than:</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">11</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">15.5</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">14</mattext></material></response_label>              <response_label ident="Res4">                <material>                  <mattext texttype="text/plain">17</mattext></material></response_label>			<response_label ident="Res5">                <material>                  <mattext texttype="text/plain">Impossible to tell</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res4</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"></displayfeedback></respcondition>           <respcondition>            <conditionvar>              <varequal respident="LID_01">Res5</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"></displayfeedback></respcondition>          </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Incorrect. 11 is the first quartile Q1 of the distribution and therefore has 25% of the shows rated below it and 75% of the shows rated above.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect. 50% of the shows have a rating greater than the median M. 15.5 is the middle of the box, half way between Q1 and Q3, but not the median in this case.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Correct. 50% of the shows have a rating greater than the median M, which according to the boxplot is 14.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack3">          <material>            <mattext texttype="text/plain">Incorrect. 50% of the shows have a rating greater than the median M. 17 is the mid-range, half way between the minimum and the Maximum, but not the median in this case.</mattext></material></itemfeedback>		<itemfeedback ident="FeedBack4">          <material>            <mattext texttype="text/plain">Incorrect. 50% of the shows have a rating greater than the median M. Where is the median located according to the boxplot? </mattext></material></itemfeedback>        </item></questestinterop>