<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="Histogram1of3DIGTq5" title="question_5">
    <presentation>
      <material>
        <mattext texttype="text/plain">How many people were surveyed?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">20</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">32</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">36</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">226</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">Incorrect. Count the frequencies instead of the phone calls.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">Incorrect. Are you looking at the histogram instead of the table?</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">Correct.  If we add the frequencies we get 36.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
