 <?xml version="1.0" encoding="UTF-8"?>
     <questestinterop>
     <item ident="stats_item_001.2" title="didigetthis_histogram1q3.xml">
        <presentation>
          <material>
            <mattext texttype="text/plain">What percentage of students study 6 or more hours for the midterm?</mattext></material>
          <response_lid rcardinality="Single" ident="LID_01">
            <render_choice>
              <response_label ident="Res1">
                <material>
                  <mattext texttype="text/plain">15%</mattext></material></response_label>
              <response_label ident="Res2">
                <material>
                  <mattext texttype="text/plain">23%</mattext></material></response_label>
              <response_label ident="Res3">
                <material>
                  <mattext texttype="text/plain">52%</mattext></material></response_label>
              <response_label ident="Res4">
                <material>
                  <mattext texttype="text/plain">60%</mattext></material></response_label>
			<response_label ident="Res5">
                <material>
                  <mattext texttype="text/plain">94%</mattext></material></response_label>
              </render_choice></response_lid></presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res1</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res2</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res3</varequal></conditionvar>
            <setvar varname="SCORE">10</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>
          <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res4</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"></displayfeedback></respcondition>
           <respcondition>
            <conditionvar>
              <varequal respident="LID_01">Res5</varequal></conditionvar>
            <setvar varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"></displayfeedback></respcondition>
          </resprocessing>
     <itemfeedback ident="hint">
      <hint>
        <hintmaterial>
          <material>
            <mattext texttype="text/plain">Use the histogram above to find the total number of students in the class, and how many students studied six or more hours. Then convert to percents.</mattext></material></hintmaterial>
   </hint></itemfeedback>
       <itemfeedback ident="FeedBack0">
          <material>
            <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
        <itemfeedback ident="FeedBack1">
          <material>
            <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
        <itemfeedback ident="FeedBack2">
          <material>
            <mattext texttype="text/plain">Correct.  Note that the data displays information about 3+9+6+3+2+1+1=25 students. Out of the 25 students, 6 + 3 + 2 + 1 + 1 = 13 studied 6 or more hours for the exam, which is 3/25 = 0.52 = 52% of the students.</mattext></material></itemfeedback>
        <itemfeedback ident="FeedBack3">
          <material>
            <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
		<itemfeedback ident="FeedBack4">
          <material>
            <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
        </item></questestinterop>
