<questestinterop>
  <item ident="StandardDeviation3of3q2" title="question_2">
    <presentation>
      <material>
        <mattext texttype="text/html">What percentage of the temperatures are less than 99.5&#176;F?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/html">0.15%</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/html">0.3%</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/html">2.5%</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/html">5%</mattext></material></response_label>
          <response_label ident="Res5">
            <material>
              <mattext texttype="text/html">16%</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
