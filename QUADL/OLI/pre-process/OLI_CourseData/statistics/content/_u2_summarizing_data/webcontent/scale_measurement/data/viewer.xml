			<backdrop displayBgSp="true"/>
			<master id="1" frameNum="0"/>
			<layout id="2" frameNum="0"/>
			<slidexml><AS3Class className="TL_dc8144ef42d24d4f9936f1685d87e705yxy" classType="PptTmline_xyxyxyxyxyxyxyxyxyxyxyxyxyxy" frameRate="30" normalSlide="1"><OnLoadCallBacks/><Audios/></AS3Class></slidexml>
			<clickAnimations>
				<click startTime="1700" isStartRelative="false" duration="534"/>
				<click startTime="3200" isStartRelative="false" duration="534"/>
				<click startTime="7300" isStartRelative="false" duration="534"/>
