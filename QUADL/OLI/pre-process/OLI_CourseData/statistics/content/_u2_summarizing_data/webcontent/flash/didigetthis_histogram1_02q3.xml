<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="Histogram1of3DIGTq3" title="question_3">
    <presentation>
      <material>
        <mattext texttype="text/plain">What proportion of the students surveyed eat exactly 5 servings of fruits and vegetables daily?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">.063</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">.094</mattext></material></response_label>
          <response_label ident="Res5">
            <material>
              <mattext texttype="text/plain">.156</mattext></material></response_label>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">2</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">3</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res5</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">Two is the number of servings that 5 students ate. The question wants the proportion of students who eat exactly 5 "servings" daily. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">Correct. 3/32 = .09375, which can be rounded to .094.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">You used 5/32 to find the proportion, but 5 is not the number of students, it is the number of servings. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
      <material>
        <mattext texttype="text/plain">Two is the number of servings that 5 students ate. The question wants the proportion who eat exactly 5 "servings" daily. Also, 2 is not a proportion. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack4">
      <material>
        <mattext texttype="text/plain">While it is true that 3 of the students eat exactly 5 servings daily, this is not a proportion. Please try again.</mattext></material></itemfeedback></item></questestinterop>
