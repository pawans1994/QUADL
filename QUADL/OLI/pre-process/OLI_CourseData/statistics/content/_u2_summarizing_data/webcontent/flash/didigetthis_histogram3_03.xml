<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="didigetthis_histogram3_03" title="question_3">
    <presentation>
      <material>
        <mattext texttype="text/plain">What do you think is the shape of the distribution of the age at which a child takes its first steps?  Why?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">Symmetric - Uniform</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">Skewed Left</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">Skewed Right</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">Symmetric - Unimodal</mattext></material></response_label>
          <response_label ident="Res5">
            <material>
              <mattext texttype="text/plain">Symmetric - Bimodal</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res5</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">Incorrect. </mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
      <material>
        <mattext texttype="text/plain">Correct. Most children start to walk at about the same age. The distribution is centered at about 18 months, has some variability, but would not have outliers. Thus, it is symmetric.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack4">
      <material>
        <mattext texttype="text/plain">Incorrect.</mattext></material></itemfeedback></item></questestinterop>
