<?xml version="1.0" encoding="UTF-8"?>
 <item ident="stats_item" title="didigetthis_center1q1">
    <presentation>
      <material>
        <mattext texttype="text/plain">Using the histogram provided, what will be the relationship between the mean and median of the collected data?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">The mean will be bigger than the median.</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">The mean will be smaller than the median.</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">The mean and median will be about the same.</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition></resprocessing>
    <itemfeedback ident="hint">
      <hint>
        <hintmaterial>
          <material>
            <mattext texttype="text/plain">As you look at the histogram decide if the distribution is skewed right, skewed left, symmetric, or uniform.</mattext></material></hintmaterial></hint></itemfeedback>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">Incorrect. As you look at the histogram, notice that the distribution is roughly symmetric.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">Incorrect. As you look at the histogram, notice that the distribution is roughly symmetric.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">Correct. Since the distribution is symmetric, the mean and median would be about the same.</mattext></material></itemfeedback></item>
