<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="Histogram1of3DIGTq1" title="question_1">
    <presentation>
      <material>
        <mattext texttype="text/plain">How many of the students surveyed eat at least 4 servings of fruits and vegetables daily?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">4</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">8</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">12</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">20</mattext></material></response_label>
          <response_label ident="Res5">
            <material>
              <mattext texttype="text/plain">28</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res5</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">You have found the number that eat more than 4 servings daily. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">You have found the number that eat exactly 4 servings daily. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">Correct. At least 4 means 4 or more, which is 8 + 3 + 1 = 12.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
      <material>
        <mattext texttype="text/plain">Your have found the number that eat less than 4 servings daily. The word "least" is often confused with less than. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack4">
      <material>
        <mattext texttype="text/plain">You have found the number that eat 4 or less servings daily. Although at least 4 does include 4, you have confused "least" with less than. Please try again.</mattext></material></itemfeedback></item></questestinterop>
