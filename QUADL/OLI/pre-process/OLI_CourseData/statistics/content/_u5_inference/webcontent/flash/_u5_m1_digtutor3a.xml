 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="Title">        <presentation>          <material>            <mattext texttype="text/plain">Which of the three confidence intervals provides the most precise estimation?</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">(3, 4)</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">(2, 5)</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">(2.5, 4.5)</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Good Job!  Indeed, since this is the narrower confidence interval, it is the one that provides the most precise estimation of the unknown mean.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect.  The narrower the confidence interval the more precise estimation it provides of the unknown mean. </mattext></material></itemfeedback>        <itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Incorrect.  The narrower the confidence interval the more precise estimation it provides of the unknown mean. </mattext></material></itemfeedback>        </item></questestinterop>