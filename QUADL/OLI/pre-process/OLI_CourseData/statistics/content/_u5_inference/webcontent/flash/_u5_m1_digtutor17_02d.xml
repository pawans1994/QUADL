    <presentation>
      <material>
        <mattext texttype="text/plain">If the p-value is 0.103, based upon the same data, which confidence interval could be the 95% confidence interval for p?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">(0.068, 0.082)</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">(0.068, 0.078)</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">(0.085, 0.096)</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">(0.098, 0.112)</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
