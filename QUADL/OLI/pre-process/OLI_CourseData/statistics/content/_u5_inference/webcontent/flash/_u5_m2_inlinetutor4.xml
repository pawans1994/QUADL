 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="Title">        <presentation>          <material>            <mattext texttype="text/plain">The expected count of females not driving drunk is:</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">(526*138)/619</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">(93* 481)/619</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">(93*138)/619</mattext></material></response_label>              <response_label ident="Res4">                <material>                  <mattext texttype="text/plain">(526*481)/619</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res4</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          </resprocessing>    <itemfeedback ident="hint">      <hint>        <hintmaterial>          <material>            <mattext texttype="text/plain">expected count = (column total * row total)/table total</mattext></material></hintmaterial>	</hint></itemfeedback>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Good Job!  Indeed 526 is the column total for this cell, and 138 is the row total.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect.  93 is not the column total for this cell, and 481 is not the row total. </mattext></material></itemfeedback>        <itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Incorrect.  93 is not the column total for this cell.  </mattext></material></itemfeedback>        <itemfeedback ident="FeedBack3">          <material>            <mattext texttype="text/plain">Incorrect.  481 is not the row total for this cell. </mattext></material></itemfeedback>        </item></questestinterop>