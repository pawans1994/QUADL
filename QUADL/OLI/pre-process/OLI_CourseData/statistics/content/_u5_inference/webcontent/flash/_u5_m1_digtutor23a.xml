  <item ident="_u5_m1_digtutor23a" title="question_1">
    <presentation>
      <material>
        <mattext texttype="text/plain">We think that the most common color for automobiles is silver and that 24% of all automobiles sold are silver. We take a random sample of 225 cars an find that 63 of them are silver. Calculate the test-statistic for this data.</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/plain">1.34</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/plain">−1.40</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/plain">1.40</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/plain">2.14</mattext></material></response_label>
          <response_label ident="Res5">
            <material>
              <mattext texttype="text/plain">2.50</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
