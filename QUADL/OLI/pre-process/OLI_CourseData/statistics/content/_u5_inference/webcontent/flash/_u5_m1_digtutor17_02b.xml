<questestinterop>
  <item ident="_u5_m1_digtutor17_02b" title="question_2">
    <presentation>
      <material>
        <mattext texttype="text/plain">At the 0.05 significance level, which conclusion can be made?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/html">Cannot reject H&lt;sub&gt;0&lt;/sub&gt;, since 0.29 falls within the confidence interval.</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/html">Reject H&lt;sub&gt;0&lt;/sub&gt;, since 0.29 falls within the confidence interval.</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/html">Reject H&lt;sub&gt;0&lt;/sub&gt;, since 0.29 does not fall within the confidence interval.</mattext></material></response_label>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/html">Cannot reject H&lt;sub&gt;0&lt;/sub&gt;, since 0.29 does not fall within the confidence interval.</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">Correct. 0.29 is between 0.273 and 0.304; thus, it is a plausible value for p and the null hypothesis cannot be rejected.</mattext></material></itemfeedback>
