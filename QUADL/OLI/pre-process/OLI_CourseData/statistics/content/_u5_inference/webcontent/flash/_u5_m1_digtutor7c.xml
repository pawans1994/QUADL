 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="Title">        <presentation>          <material>            <mattext texttype="text/plain">If you were to use a random sample of size n=640 US adults (instead of what you found in question 1), what would the margin of error roughly be?</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">4%</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">.156%</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">3%</mattext></material></response_label>              <response_label ident="Res4">                <material>                  <mattext texttype="text/plain">5%</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res4</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"></displayfeedback></respcondition>          </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Good Job!  Indeed, using the conservative approach, the margin of error would roughly be 1/sqrt(640) = .0395, which equals approximately .04 </mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect.  It seems like you forgot to take the square root in the denominator. </mattext></material></itemfeedback>        <itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Incorrect.  Recall that (conservatively) m=1/sqrt(n). </mattext></material></itemfeedback>        <itemfeedback ident="FeedBack3">          <material>            <mattext texttype="text/plain">Incorrect.  Recall that (conservatively) m=1/sqrt(n). </mattext></material></itemfeedback>        </item></questestinterop>