<?xml version="1.0" encoding="UTF-8"?>

<questestinterop>
  <item ident="_u5_m1_digtutor17_02a" title="question_1">
    <presentation>
      <material>
        <mattext texttype="text/plain">At the .05 level of significance, which conclusion can be made?</mattext></material>
      <response_lid rcardinality="Single" ident="LID_01">
        <render_choice>
          <response_label ident="Res4">
            <material>
              <mattext texttype="text/html">Fail to reject H&lt;sub&gt;o&lt;/sub&gt; since .37 falls within the limits of the interval.</mattext></material></response_label>
          <response_label ident="Res3">
            <material>
              <mattext texttype="text/html">Reject H&lt;sub&gt;o&lt;/sub&gt; since .37 falls within the limits of the interval.</mattext></material></response_label>
          <response_label ident="Res1">
            <material>
              <mattext texttype="text/html">Reject H&lt;sub&gt;o&lt;/sub&gt; since .37 does not fall within the limits of the interval.</mattext></material></response_label>
          <response_label ident="Res2">
            <material>
              <mattext texttype="text/html">Fail to reject H&lt;sub&gt;o&lt;/sub&gt; since .37 does not fall within the limits of the interval.</mattext></material></response_label></render_choice></response_lid></presentation>
    <resprocessing>
      <outcomes>
        <decvar varname="SCORE" vartype="integer"/></outcomes>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res4</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res3</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res1</varequal></conditionvar>
        <setvar varname="SCORE">10</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"/></respcondition>
      <respcondition>
        <conditionvar>
          <varequal respident="LID_01">Res2</varequal></conditionvar>
        <setvar varname="SCORE">0</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"/></respcondition></resprocessing>
    <itemfeedback ident="FeedBack0">
      <material>
        <mattext texttype="text/plain">Incorrect. .37 is not between .25 and .32. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack1">
      <material>
        <mattext texttype="text/plain">Incorrect. .37 is not between .25 and .32. Please try again.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack2">
      <material>
        <mattext texttype="text/plain">Correct. Since .37 is not between .25 and .32, it is not a plausible value for p, and the null hypothesis would be rejected.</mattext></material></itemfeedback>
    <itemfeedback ident="FeedBack3">
      <material>
        <mattext texttype="text/plain">Incorrect. If .37 does not fall within the limits of the interval, it is not a plausible value for p. Please try again.</mattext></material></itemfeedback></item></questestinterop>
