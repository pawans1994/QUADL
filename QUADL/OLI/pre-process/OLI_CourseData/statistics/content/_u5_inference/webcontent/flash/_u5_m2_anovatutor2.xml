 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="Title">        <presentation>          <material>            <mattext texttype="text/plain">Look carefully at the graphs of both scenarios. For which of the two scenarios would you be willing to believe that samples have been taken from four groups which have the same population means?    </mattext>	</material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">Scenario 1</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">Scenario 2</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          </resprocessing> 	<itemfeedback ident="hint">      <hint>        <hintmaterial>          <material>            <mattext texttype="text/plain">Where do you see more overlap between the boxes?</mattext></material></hintmaterial>          </hint></itemfeedback>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Good Job!  Your intuition is right. Read on to understand what's going on here.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect.  It is OK, though. This question was just to get you thinking about this. Read on to understand what going on here.</mattext></material></itemfeedback>        </item></questestinterop>