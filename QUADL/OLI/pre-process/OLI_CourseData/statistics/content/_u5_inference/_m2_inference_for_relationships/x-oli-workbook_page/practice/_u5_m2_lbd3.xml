                                    <m:msub>
                                        <m:mi>&mu;</m:mi>
                                        <m:mn>2</m:mn>
                                    </m:msub>
                                    <m:mo>:</m:mo>
                                    <m:mtext>Difference between two means</m:mtext>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd>
