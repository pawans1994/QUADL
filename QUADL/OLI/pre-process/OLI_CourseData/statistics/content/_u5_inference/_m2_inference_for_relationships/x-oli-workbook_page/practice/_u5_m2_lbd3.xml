                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd>
                                    <m:msub>
                                        <m:mi>H</m:mi>
                                        <m:mi>a</m:mi>
                                    </m:msub>
                                    <m:mo>:</m:mo>
                                    <m:msub>
                                        <m:mi>&mu;</m:mi>
                                        <m:mn>1</m:mn>
                                    </m:msub>
