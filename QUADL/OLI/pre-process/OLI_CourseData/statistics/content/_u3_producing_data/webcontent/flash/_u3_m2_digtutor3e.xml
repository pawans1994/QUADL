          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res4</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack3"></displayfeedback></respcondition>         <respcondition>            <conditionvar>              <varequal respident="LID_01">Res5</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack4"></displayfeedback></respcondition>         </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">That’s not right.  There are 30 students for 6 treatments....now you do the math.	</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">That’s not right.  There are 30 students for 6 treatments....now you do the math.</mattext></material></itemfeedback>         <itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Correct.  Indeed, there are 30 students and 6 treatments, so there should be 30/6=5 students in each treatment groups.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack3">          <material>            <mattext texttype="text/plain">That’s not right.  There are 30 students for 6 treatments....now you do the math.</mattext></material></itemfeedback>         <itemfeedback ident="FeedBack4">          <material>            <mattext texttype="text/plain">That’s not right.  There are 30 students for 6 treatments....now you do the math.</m