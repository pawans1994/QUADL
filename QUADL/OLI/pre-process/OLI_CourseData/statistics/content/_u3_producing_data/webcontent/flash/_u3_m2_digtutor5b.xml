 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="_u3_m2_digtutor5b">        <presentation>          <material>            <mattext texttype="text/plain">For which of the following questions would we expect the most honest responses?</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">How tall are you?</mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">How many times in your life have you used illegal drugs?</mattext></material></response_label>              <response_label ident="Res3">                <material>                  <mattext texttype="text/plain">What is the lowest course grade you received last year?</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res3</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack2"></displayfeedback></respcondition>         </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Good Job!  Indeed, this is the question that is the least sensitive.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Incorrect. Which of the questions is the least sensitive?</mattext></material></itemfeedback><itemfeedback ident="FeedBack2">          <material>            <mattext texttype="text/plain">Incorrect. Which of the questions is the least sensitive?</mattext></material></itemfeedback>       </item></questestinterop>