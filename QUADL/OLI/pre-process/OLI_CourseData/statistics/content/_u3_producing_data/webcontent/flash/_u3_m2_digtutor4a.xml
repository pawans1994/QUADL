 <?xml version="1.0" encoding="UTF-8"?>     <questestinterop>     <item ident="stats_item_001.2" title="_u3_m2_digtutor4a">        <presentation>          <material>            <mattext texttype="text/plain">Which of these is a matched pairs design?			</mattext></material>          <response_lid rcardinality="Single" ident="LID_01">            <render_choice>              <response_label ident="Res1">                <material>                  <mattext texttype="text/plain">Randomly assign the underarms of some subjects to be injected with Botox, and those of other subjects with salt water.				 </mattext></material></response_label>              <response_label ident="Res2">                <material>                  <mattext texttype="text/plain">For each subject, one underarm is injected with Botox, and the other with salt water.</mattext></material></response_label>              </render_choice></response_lid></presentation>        <resprocessing>          <outcomes>            <decvar varname="SCORE" vartype="integer"></decvar></outcomes>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res1</varequal></conditionvar>            <setvar varname="SCORE">0</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack0"></displayfeedback></respcondition>          <respcondition>            <conditionvar>              <varequal respident="LID_01">Res2</varequal></conditionvar>            <setvar varname="SCORE">10</setvar>            <displayfeedback feedbacktype="Response" linkrefid="FeedBack1"></displayfeedback></respcondition>         </resprocessing>        <itemfeedback ident="FeedBack0">          <material>            <mattext texttype="text/plain">Incorrect. This is just a regular experiment with two treatments.</mattext></material></itemfeedback>        <itemfeedback ident="FeedBack1">          <material>            <mattext texttype="text/plain">Good Job!  Indeed, this is a matched pairs design. The same person is getting both treatments.</mattext></material></itemfeedback>       </item></questestinterop>